{
    "manifest_version": 2,
    "name": "Selenium.Modeler",
    "description": "Selenium.Modeler privdes models in JS and C# for PageObjectPattern",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.4.1.min.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": { "16": "icon.png" },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "chrome://favicon/",
        "storage"
    ]
}