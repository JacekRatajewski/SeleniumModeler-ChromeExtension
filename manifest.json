{
    "manifest_version": 2,
    "name": "Selenium.Modeler",
    "description": "Selenium.Modeler - Page Object Pattern DOM Elements provider.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "query.bundle.js",
                "modeler.bundle.js",
                "cursor.bundle.js",
                "pip.bundle.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "assets/SeMLogo.png",
        "default_popup": "core.html"
    },
    "icons": { "16": "assets/SeMLogo.png" },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "chrome://favicon/",
        "storage"
    ]
}